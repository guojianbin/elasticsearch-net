    <PackageReference Include="JetBrains.Profiler.Kernel.Windows.Api" Version="108.0.20170209.151431-eap01" />
  </ItemGroup>
  <ItemGroup Condition="'$(TargetFramework)'!='netcoreapp1.0'">
